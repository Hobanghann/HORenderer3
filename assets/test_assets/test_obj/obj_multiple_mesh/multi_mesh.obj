# multi_mesh.obj
mtllib multi_mesh.mtl

# =========================
# Mesh A : Metal (Cube-like)
# =========================
o Mesh_Metal_A
usemtl metal

v 0 0 0
v 1 0 0
v 1 1 0
v 0 1 0

vn 0 0 1

vt 0 0
vt 1 0
vt 1 1
vt 0 1

f 1/1/1 2/2/1 3/3/1
f 1/1/1 3/3/1 4/4/1

# =========================
# Mesh B : Plastic (Plane)
# =========================
o Mesh_Plastic
usemtl plastic

v 2 0 0
v 3 0 0
v 3 1 0
v 2 1 0

vn 0 0 1

vt 0 0
vt 1 0
vt 1 1
vt 0 1

f 5/5/2 6/6/2 7/7/2
f 5/5/2 7/7/2 8/8/2

# =========================
# Mesh C : Metal again
# (material reuse test)
# =========================
o Mesh_Metal_B
usemtl metal

v 0 0 1
v 1 0 1
v 1 1 1
v 0 1 1

vn 0 0 -1

vt 0 0
vt 1 0
vt 1 1
vt 0 1

f 9/9/3 10/10/3 11/11/3
f 9/9/3 11/11/3 12/12/3
